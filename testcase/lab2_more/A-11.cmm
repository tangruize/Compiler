// A-11
//输入
 int bbSort(int n)
 {
 int exchange;
 int i, j, temp;
 int p[50];
 while(i < n)
 {
 i = i + 1;
 exchange = 1;
 while(j < n)
 {
 j = j + 1;
 if(p[j] > p[j+1])
 {
 temp = p[j+1];
 p[j+1] = p[j];
 p[j] = temp;
 exchange = 0;
 }
 }
 if(exchange() == 0)
 p[i] = -1;
 }
 return 0;
 }
//输出
// Error type 11 at line 22: "exchange" must be a function
//说明:对非函数的标识符使用 () 操作符。
